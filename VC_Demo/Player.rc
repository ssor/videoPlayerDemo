IDR_MAINFRAME MENU DISCARDABLE 
BEGIN
    POPUP "File(&F)"
    BEGIN
        MENUITEM "Open...     Ctrl+O",          IDM_FILE_OPEN
        MENUITEM "Close...    Ctrl+C",          IDM_FILE_CLOSE
        MENUITEM SEPARATOR
        MENUITEM "Exit(&X)",                    IDM_APP_EXIT
    END
    POPUP "View(&V)"
    BEGIN
        MENUITEM "FullScreen(&S)\tF2",          IDM_VIEW_FULLSCREEN
        POPUP "Video size(&Z)"
        BEGIN
            MENUITEM "50%",                         IDM_VIEW_ZOOM_50
            MENUITEM "100%",                        IDM_VIEW_ZOOM_100
            , CHECKED
            MENUITEM "200%",                        IDM_VIEW_ZOOM_200
        END
        MENUITEM SEPARATOR
        MENUITEM "Information",                 IDM_INFO
        MENUITEM SEPARATOR
        MENUITEM "Multi Display",               IDM_SETDISPLAY
    END
    POPUP "Control(&C)"
    BEGIN
        MENUITEM "Play/Pause     Space",        IDM_PLAY_PAUSE
        MENUITEM "Stop           .",            IDM_STOP
        MENUITEM "Step Forward   -->",          IDM_STEPFORWARD
        MENUITEM "Step Bacward   <--",          IDM_STEPBACKWARD
        MENUITEM "Goto Start     Ctrl+<--",     IDM_GOTOSTART
        MENUITEM "Goto End       Ctrl+-->",     IDM_GOTOEND
        MENUITEM "Locate To      Ctrl+L",       IDM_SEEK
        MENUITEM SEPARATOR
        MENUITEM "Video Control",               IDM_VIDEO_CONTROL
        MENUITEM SEPARATOR
        MENUITEM "Rewind",                      IDM_REPEAT
    END
    POPUP "Option(&P)"
    BEGIN
        MENUITEM "High Image Quality",          IDM_QUALITY, CHECKED
        POPUP "Throw B-Frame"
        BEGIN
            MENUITEM "Don't throw",                 IDM_THROW0
            , CHECKED, HELP
            MENUITEM "Throw 1 B-Frame",             IDM_THROW1, HELP
            MENUITEM "Throw 2 B-Frame",             IDM_THROW2, HELP
        END
        MENUITEM SEPARATOR
        POPUP "Display type"
        BEGIN
            MENUITEM "Normal",                      IDM_DISNORMAL, CHECKED
            MENUITEM "Quarter",                     IDM_DISQUARTER
        END
        POPUP "Timer Select"
        BEGIN
            MENUITEM "Timer_1",                     IDM_TIMER1, CHECKED
            MENUITEM "Timer_2",                     IDM_TIMER2
        END
        MENUITEM "Reset Buffer",                IDM_RESET_BUF
        MENUITEM SEPARATOR
        POPUP "Cap Pic Type"
        BEGIN
            MENUITEM "BMP",                         IDM_CAP_BMP, CHECKED
            MENUITEM "JPEG",                        IDM_CAP_JPEG, GRAYED
        END
        MENUITEM "Set Cap Pic Path",            IDM_CAPPIC_PATH
    END
    POPUP "Help(&H)"
    BEGIN
        MENUITEM "Help...",                     IDM_APP_HELP
        MENUITEM "About...",                    IDM_APP_ABOUT
    END
END


